@model IEnumerable<object>

@{
    ViewData["Title"] = "Giỏ hàng";
}


<div class="page-head">
    <div class="page-title">Giỏ hàng</div>
</div>

@if (Model == null || !Model.Any())
{
     @Html.Partial("_EmptyCart") 
}
else
{
    <div class="tool-card">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>Sản phẩm</th>
                    <th>Giá</th>
                    <th>Số lượng</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>Product name</td>
                        <td>Price</td>
                        <td>Qty</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}


